{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js"]
  },
  "rules": {
    "no-console": {
      "severity": "warn"
    },
    "object-literal-sort-keys": false,
    "ordered-imports": {
      "severity": "warn"
    }
  }
}
